{
  "name": "vite-react-typescript",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Vite React TypeScript boilerplate",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/shopot/vite-react-typescript"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "tsc --noEmit && eslint ./src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint ./src --ext ts,tsx --fix",
    "preview": "vite preview",
    "prepare": "husky install",
    "lintstaged": "lint-staged --relative"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix"
    ],
    "*.css": [
      "prettier --write -config .prettierrc"
    ]
  },
  "dependencies": {
    "@types/compose-function": "^0.0.31",
    "compose-function": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0"
  },
  "devDependencies": {
    "@types/node": "^20.6.3",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "@vitejs/plugin-react": "^4.0.4",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "sass": "^1.67.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.9"
  }
}
